{
  "compilerOptions": {
    "sourceMap": true,
    "target": "es5",
    "jsx": "react",
    "module": "es6",
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "declaration": false,
    "removeComments": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "strict": true,
    "outDir": "build",
    "lib": ["es6", "es7", "dom"],
    "baseUrl": ".",
    "paths": {
      "app/*": ["./src/app/*"],
      "assets/*": ["./src/assets/*"],
      "canner-schema": ["./canner.schema"],
      "canner-resolver": ["./canner.resolver"],
      "canner-connector": ["./canner.connector"]
    }
  },
  "exclude": ["dist", "build", "node_modules"]
}
